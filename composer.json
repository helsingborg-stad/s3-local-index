{
    "name": "helsingborg-stad/s3-local-index",
    "type": "wordpress-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian Thulin",
            "email": "sebastian.thulin@helsingborg.se"
        }
    ],
    "require": {
        "helsingborg-stad/wpservice": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "helsingborg-stad/phpcs": "^0.2.2"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "S3_Local_Index\\": "source/php/",
            "S3LocalIndex\\": "source/php/"
        }
    },
    "version": "0.1.20"
}